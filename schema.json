{
  "$id": "https://example.com/fstab",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "$defs": {
    "providers": {
      "$anchor": "NotificationProvider",
      "type": "string",
      "anyOf": [
        {
          "const": "pushover"
        },
        {
          "const": "slack"
        },
        {
          "const": "pushbullet"
        },
        {
          "const": "gi"
        },
        {
          "const": "telegram"
        },
        {
          "const": "ntfy"
        },
        {
          "const": "console"
        },
        {
          "const": "console_async"
        }
      ]
    },
    "doctor_locators": {
      "$anchor": "DoctorLocator",
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "enabled": {
          "type": "boolean"
        }
      },
      "required": [
        "id",
        "name",
        "enabled"
      ]
    }
  },
  "properties": {
    "luxmedsniper": {
      "properties": {
        "doctor_locators": {
          "type": "array",
          "items": {
            "$ref": "#DoctorLocator"
          }
        },
        "lookup_time_days": {
          "type": "number"
        },
        "notification_provider": {
          "type": "array",
          "items": {
            "$ref": "#NotificationProvider"
          },
          "uniqueItems": true
        },
        "facilities_ids": {
          "type": "array",
          "items": {
            "type": "number"
          },
          "uniqueItems": true
        }
      },
      "required": [
        "doctor_locators",
        "lookup_time_days",
        "notification_provider"
      ],
      "additionalProperties": false
    },
    "misc": {
      "type": "object",
      "properties": {
        "notifydb_template": {
          "type": "string"
        }
      },
      "required": [
        "notifydb_template"
      ]
    },
    "luxmed": {
      "properties": {
        "email": {
          "type": "string"
        },
        "password": {
          "type": "string"
        }
      },
      "required": [
        "email",
        "password"
      ]
    },
    "pushover": {
      "properties": {
        "user_key": {
          "type": "string"
        },
        "api_token": {
          "type": "string"
        },
        "message_template": {
          "type": "string"
        },
        "title": {
          "type": "string"
        }
      }
    },
    "slack": {
      "properties": {
        "channel": {
          "type": "string"
        },
        "api_token": {
          "type": "string"
        },
        "message_template": {
          "type": "string"
        }
      }
    },
    "pushbullet": {
    },
    "gi": {
    },
    "telegram": {
    },
    "ntfy": {
      "properties": {
        "topic": {
          "type": "string"
        },
        "message_template": {
          "type": "string"
        }
      }
    },
    "console": {
      "properties": {
        "message_template": {
          "type": "string"
        }
      }
    },
    "console_async": {
      "properties": {
        "message_template": {
          "type": "string"
        }
      }
    }
  },
  "required": [
    "luxmedsniper",
    "misc",
    "luxmed"
  ],
  "additionalProperties": false
}
